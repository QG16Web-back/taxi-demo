    <!--映射实体对象-->
    <resultMap id="GPSPointResultMap" type="com.qg.taxi.model.GPSPoint">
        <id property="id" column="ID"/>
        <result property="licenseplateno" column="LICENSEPLATENO"/>
        <result property="gps_date" column="GPS_TIME" javaType="java.util.Date"/>
        <result property="longitude" column="LONGITUDE"/>
        <result property="latitude" column="LATITUDE"/>
        <result property="speed" column="SPEED"/>
        <result property="car_stat1" column="CAR_STAT1"/>
    </resultMap>

    <insert id="addPointList11" parameterType="com.qg.taxi.model.GPSPoint">
        INSERT INTO gpsdata11(GEOHASH,LICENSEPLATENO,GPS_TIME,LONGITUDE,LATITUDE,SPEED,CAR_STAT1)
        VALUES
        <foreach collection="list" item="item" index="index" separator=",">
            (
            (SELECT ST_GEOHASH(#{item.longitude},#{item.latitude}, 7)),
            #{item.licenseplateno},
            #{item.gps_date},
            #{item.longitude},
            #{item.latitude},
            #{item.speed},
            #{item.car_stat1}
            )
        </foreach>
    </insert>

